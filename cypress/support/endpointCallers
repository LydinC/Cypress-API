export const getCountries = () => {
    cy.request({
        method: 'GET',
        url: '/countries',
        headers: {
          accept: 'application/json',
          'X-RapidAPI-Key': Cypress.env("X-RapidAPI-Key"),
          'X-RapidAPI-Host': Cypress.env("X-RapidAPI-Host")
        },
        // qs:{
        //     search: country
        // },
        failOnStatusCode: false
    })   
}